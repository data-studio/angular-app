#!/usr/bin/env node
'use strict';

const exec = require('child_process').exec;
const path = require('path');
const fs = require('fs');

exec('./node_modules/gulp/bin/gulp.js', { cwd: path.resolve(__dirname, '..') }, (error, stdout, stderr) => {
  if (error) {
    return console.error(`exec error: ${error}`);
  }
}).stdout.pipe(process.stdout);
